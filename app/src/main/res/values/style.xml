<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="RoundFAB" parent="Widget.MaterialComponents.FloatingActionButton">
        <item name="shapeAppearanceOverlay">@style/RoundShapeOverlay</item>
    </style>

    <style name="RoundShapeOverlay">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">50%</item>
    </style>

    <style name="RoundedBottomCorners" parent="">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSizeTopLeft">0dp</item>
        <item name="cornerSizeTopRight">0dp</item>
        <item name="cornerSizeBottomLeft">24dp</item>
        <item name="cornerSizeBottomRight">24dp</item>
    </style>

    <style name="TimeSlotButton">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_columnWeight">1</item>
        <item name="android:backgroundTint">#E0E0E0</item>
    </style>

    <style name="CalendarDateBlack" parent="TextAppearance.AppCompat">
        <item name="android:textColor">#000000</item>
    </style>


</resources>

